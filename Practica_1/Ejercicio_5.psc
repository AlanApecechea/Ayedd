Algoritmo hipotenusa_triangulo_rectangulo
	//Definición de varibales.
	Definir a,b Como Real;
	a <- 0;
	b <- 0;
	
	//Calculo de la hipotenusa de un triángulo rectángulo.
	Escribir "Ingrese el valor del primer cateto";
	Leer a;
	Escribir "Ingrese el valor del segundo cateto";
	Leer b;
	hipotenusa <- (a^2 + b^2) ^ (1/2);
	Escribir "El resultado de la hipotenusa es ", hipotenusa;
	
FinAlgoritmo
